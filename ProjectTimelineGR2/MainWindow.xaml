<Window x:Class="ProjectTimelineGR2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjectTimelineGR2"
        mc:Ignorable="d" Loaded="Window_Loaded"
        Title="MainWindow" Height="550" Width="1100">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="201*"/>
            <ColumnDefinition Width="599*"/>
        </Grid.ColumnDefinitions>
        <StackPanel Background="DarkCyan">
            <Button x:Name="btnCapture" Content="Capture Project" FontSize="20" Height="45" Margin="10 25" Click="btnCapture_Click"/>
            <Button x:Name="btnAssignProject" Content="Assign Project" FontSize="20" Height="45" Margin="10 25" Click="btnAssignProject_Click"/>
            <Button x:Name="btnDisplay" Content="Display" FontSize="20" Height="45" Margin="10 25" Click="btnDisplay_Click"/>
            <Button x:Name="btnFilter" Content="Filter" FontSize="20" Height="45"  Margin="10 25" Click="btnFilter_Click" />

        </StackPanel>
        <Frame Grid.Column="1" Name="frmContainer" Padding="15" NavigationUIVisibility="Hidden"/>

    </Grid>
</Window>
